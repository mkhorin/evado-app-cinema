
<template data-id="pagination">
    <nav>
        <ul class="pagination">
            <li class="page-item"><a class="page-link" href="#" data-action="first"><span>«</span></a></li>
            <li class="page-item"><a class="page-link" href="#" data-action="prev"><span>‹</span></a></li>
            {{pages}}
            <li class="page-item"><a class="page-link" href="#" data-action="next"><span>›</span></a></li>
            <li class="page-item"><a class="page-link" href="#" data-action="last"><span>»</span></a></li>
        </ul>
    </nav>
</template>

<template data-id="page">
    <li class="page-item {{active}}"><a class="page-link" href="#" data-page="{{page}}">{{text}}</a></li>
</template>